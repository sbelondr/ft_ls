mkdir dirTest12
cd dirTest12

cp ../../ft_ls .

touch a b c
chmod 644 a
chmod 755 b
chmod 311 c
echo "==========="
ls -l
echo "==========="
./ft_ls -l
echo "==========="

chmod -R 777 *
rm -rf a b c

mkdir a b c
chmod 644 a
chmod 755 b
chmod 311 c
echo "==========="
ls -l
echo "==========="
./ft_ls -l
echo "==========="

chmod -R 777 *
rm -rf a b c

cd ..
rm -rf dirTest12
